{
    "Name": "three circular cylinders in equilateral-triangular arrangements",
    "ShortName":"flow past",
    "Models":
    {
         "equations": "Navier-Stokes"
    },
    "Materials":
    {
        "omega":
        {
            "rho":"1",
            "mu":"1"
        }
    },
    "Parameters":
    {
      "Re":"100",
      "D":"1.5"
    },
    "BoundaryConditions":
    {
        "velocity":
        {
            "Dirichlet":
            {
                // tag::inlet[]
                "Inlet":
                {
                    "expr":"{Re/D,0}:Re:D"
                },
                // end::inlet[]
                // tag::load[]
                "loads":
                {
                    "expr":"{0,0}",
		    "markers":["load1","load2","load3"]
                }
		// end::load[]
            }
            // "Neumann_scalar":
            // {
	    // 	// tag::outlet[]
	    // 	"Outlet":
            //     { 
            //         "expr":"0"
            //     }
            // }
            // // end::outlet[]
        },
	"velocity_y":
        {
            "Dirichlet":
            {
		"walls":
		{
		    "expr":"0",
		    "markers":["Wall1","Wall2"]
		}
	    }
	},
	"fluid":
	{
	    "outlet":{ "Outlet":{} }
	}
    },
    "PostProcess":
    {   
       // tag::process[]
        "Exports":
        {
            "fields":["velocity","vorticity","pressure","pid"]
        },
        "Measures":
        {
            "Forces":["load1","load2","load3"]
        }
        // end::process[]
    }
}
