{
    "Name": "three circular cylinders in equilateral-triangular arrangements",
    "ShortName":"flow past",
    "Models":
    {
         "equations": "Navier-Stokes"
    },
    "Materials":
    {
        "omega":
        {
            "rho":"1",
            "mu":"1"
        }
    },
    "Parametres":
    {
      "Re":"100",
      "D":"1.5"
    },
    "BoundaryConditions":
    {
        "velocity":
        {
            "Dirichlet":
            {
                // tag::inlet[]
                "Inlet":
                {
                   "expr":"{Re/D,0}:Re:D"
                },
                // end::inlet[]
                // tag::load[]
                "load1":
                {
                  "expr":"{0,0}"
                },
                "load2":
                {
                  "expr":"{0,0}"
                },
                "load3":
                {
                  "expr":"{0,0}"
                }
               // end::load[]
             },
            "outlet":
            {
             // tag::walls[]
                 "Wall1":
                 {
                     "expr":"0"
                 },
                 "Wall2":
                 {
                     "expr":"0"
                 }
             // end::walls[]
            },
          
          "Neumann_scalar":
           {
          // tag::outlet[]
               "Outlet":
                { 
                    "expr":"0"
                }
           }
          // end::outlet[]

        }
     "PostProcess":
    {   
       // tag::process[]
        "Exports":
        {
            "fields":["velocity","vorticity","pressure","pid"]
        },
        "Measures":
        {
            "Forces":["load1","load2","load3"]
        }
        // end::process[]
    }
}
}
